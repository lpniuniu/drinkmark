OTHER_LDFLAGS = $(inherited) -framework "Alamofire" -framework "CocoaLumberjack" -framework "DKChainableAnimationKit" -framework "SnapKit" -framework "ValueCoding" -framework "YapDatabase" -framework "YapDatabaseExtensions"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-drinkmark
PODS_ROOT = ${SRCROOT}/Pods